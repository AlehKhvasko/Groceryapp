{
    "owned": {
        "/owned/:userId": {
            "requiresAuth": true,
            "operations": {
                "GET": {
                    "description": "Fetches all ingredients owned by the user passed in the route parameter",
                    "req.body": "N/A",
                    "returns": [{"ingredientId": 10016049, "name": "cannellini beans"},{"ingredientId": 93604,"name": "curry leaves"}, "..."]
                },
                "POST": {
                    "description": "Adds ingredientId's from req.body to the users owned list",
                    "req.body": {"ingredientId": [10016049,437,633,"..."]},
                    "returns": "{ingredientId.length} ingredients added to your list"
                },
                "DELETE": {
                    "description": "Deletes all ingredients owned from users list",
                    "req.body": "N/A",
                    "returns": "All users ingredients deleted"
                }
            }
        },
        "/owned/:userId/:ingredientId": {
            "requiresAuth": true,
            "operations": {
                "GET": {
                    "description": "Fetches selected ingredient owned by the user",
                    "req.body": "N/A",
                    "returns": [{"ingredientId": 10016049, "name": "cannellini beans"}]
                },
                
                "DELETE": {
                    "description": "Deletes all ingredients owned from users list",
                    "req.body": "N/A",
                    "returns": "All users ingredients deleted"
                }
            }
        }
    },
    "user": {
        "/user/login": {
            "requiresAuth": false,
            "operations": {
                "POST": {
                    "description": "Logs user into app and creates cookie with the JWT",
                    "req.body": {"username": "fakeuser", "password": "password123"},
                    "returns": "Successfully logged in"
                }
            }
        },
        "/user/logout": {
            "requiresAuth": false,
            "operations": {
                "GET": {
                    "description": "Logs user out and deletes cookie containing the JWT",
                    "req.body": "N/A",
                    "returns": "Successfully logged out"
                }
            }
        },
        "/user/register": {
            "requiresAuth": false,
            "operations": {
                "POST": {
                    "description": "Registers user account and creates cookie with the JWT",
                    "req.body": {"username": "fakeuser", "password": "password123"},
                    "returns": "Registration Successful!"
                }
            }
        }
    },
